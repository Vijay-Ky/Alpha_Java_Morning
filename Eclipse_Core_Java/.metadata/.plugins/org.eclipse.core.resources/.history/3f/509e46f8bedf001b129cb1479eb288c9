package app7;

import java.util.ArrayList;

class A 
{
	public static void main(String[] args) 
	{
		ArrayList list = new ArrayList();
		list.add(10);
		list.add(20);
		list.add(30);
		list.add(40);
		for(Object i : list)
		{
			System.out.println(i);
		}
	}
}
//for-each or enhanced for loop introduced in JDK1.5
