package app3;

class A
{
	public static void main(String[] args)
	{
		int i = 0;
		System.out.println(i++);//
		System.out.println(i);//
		System.out.println(i);//
	}
}

//unary operator
//increment - post and pre syntax: variable++, ++variable
//decrement - post and pre syntax: variable--, --variable
