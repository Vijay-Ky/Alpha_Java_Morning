package app17;

class M23 {

	public static void main(String[] args) throws InterruptedException{
		System.out.println("main begin");
		for(int i = 1; i <= 10; i++)
		{
			System.out.println(i);
			
		
				//sleep method is a static method
				Thread.sleep(1000);
			
		}
		System.out.println("main end");
	}
}